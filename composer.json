{
    "name": "zhaotuqi/cms",
    "description": "公共类库-CMS使用库",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "zhaoli",
            "email": "zhaoli@163.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-curl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-mbstring": "*",
        "ext-xmlwriter":"*",
        "ext-simplexml":"*",
        "ext-fileinfo": "*",
        "php": ">=5.4.0",
        "predis/predis": "^1.1",
        "zhaotuqi/common":"dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
